<%provide(:title, 'Enter Team Information') %>
<h1>Enter Team Records</h1>
<%=form_for(@record) do |f| %>
	<%= f.label :sport %> <br />
	<%= f.text_field :sport %><br />

	<%= f.label :team %> <br />
	<%= f.text_field :team %><br />
	
	<%= f.label :teamScore %> <br />
	<%= f.text_field :teamScore %><br />

	<%= f.label :opponent %> <br />
	<%= f.text_field :opponent %><br />

	<%= f.label :opponentScore %> <br />
	<%= f.text_field :opponentScore %><br />

	<%= f.submit "Submit Team Information" %><br />
<%end%>